{
  "name": "authorization",
  "version": "1.0.0",
  "description": "Authorization Service BPP",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "pkg-check": "npm-check -s -u",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "find-deadcode": "ts-prune",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "dev": "NODE_ENV=development nodemon -L --config nodemon.json",
    "start": "node dist/infra/configs/startup",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:unit": "npm test -- --watch --silent -c jest-unit-config.js",
    "test:unit:log": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch --silent -c jest-integration-config.js",
    "test:integration:log": "npm test -- --watch -c jest-integration-config.js",
    "test:integration:log:nock": "DEBUG=nock.* npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage --silent"
  },
  "repository": {
    "type": "git",
    "url": "git@ssh.dev.azure.com:v3/BPP-BR/Refatora%C3%A7%C3%A3o%20F%C3%AAnix/authorization"
  },
  "keywords": [],
  "author": "Fernando Tomio",
  "license": "ISC",
  "devDependencies": {
    "@types/auth0": "^2.34.10",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/newrelic": "^7.0.3",
    "@types/node": "^17.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "nock": "^13.2.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@ory/kratos-client": "^0.9.0-alpha.3",
    "auth0": "^2.37.0",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "hpp": "^0.2.3",
    "http-graceful-shutdown": "^3.1.5",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "module-alias": "^2.2.2",
    "mongoose": "^6.3.6",
    "newrelic": "^8.7.0",
    "nocache": "^3.0.1",
    "rimraf": "^3.0.2",
    "superagent-proxy": "^3.0.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "xss-clean": "^0.1.1"
  }
}
